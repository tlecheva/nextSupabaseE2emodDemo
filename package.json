    "concurrently": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "prettier": "3.3.2",
    "tailwindcss": "^3.4.3"
  }
